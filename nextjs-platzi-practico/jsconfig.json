{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@pages/*": ["pages/*"],
      "@components/*": ["components/*"],
      "@containers/*": ["containers/*"],
      "@icons/*": ["assets/icons/*"],
      "@logos/*": ["assets/logos/*"],
      "@context/*": ["context/*"],
      "@styles/*": ["styles/*"],
      "@hooks/*": ["hooks/*"],
    }
  },
  "exclude": ["node_modules", "**/node_modules/*", "**/node_modules/**"]
}
